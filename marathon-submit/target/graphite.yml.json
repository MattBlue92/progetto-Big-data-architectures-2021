{
    "instances": 1, 
    "container": {
        "docker": {
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "servicePort": 3000
                }, 
                {
                    "protocol": "tcp", 
                    "containerPort": 80, 
                    "servicePort": 3080
                }, 
                {
                    "protocol": "udp", 
                    "containerPort": 8125, 
                    "servicePort": 8125
                }
            ], 
            "image": "mikljohansson/graphite:latest", 
            "network": "BRIDGE"
        }, 
        "type": "DOCKER"
    }, 
    "mem": 500, 
    "healthChecks": [
        {
            "gracePeriodSeconds": 150, 
            "portIndex": 0, 
            "protocol": "HTTP", 
            "timeoutSeconds": 5, 
            "maxConsecutiveFailures": 3, 
            "path": "/", 
            "intervalSeconds": 10
        }, 
        {
            "gracePeriodSeconds": 150, 
            "portIndex": 1, 
            "protocol": "HTTP", 
            "timeoutSeconds": 5, 
            "maxConsecutiveFailures": 3, 
            "path": "/", 
            "intervalSeconds": 10
        }
    ], 
    "cpus": 0.1, 
    "id": "/demo/graphite"
}